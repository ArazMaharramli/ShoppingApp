{
  "JwtDefaults": {
    "ValidateIssuer": true,
    "ValidateAudience": true,
    "ValidAudience": "<Your_Audience>",
    "ValidIssuer": "<Your_Issuer>",
    "SecurityKey": "<Your_Securitykey>",
    "TokenLifeTime": "Your_Token_Lifetime(hh:mm:ss)"
  },
  "ApiSettings": {
    "ApiKey": "<Your_ApiKey>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
