@model ShoppingApp.Web.UI.Areas.Admin.ViewModels.ColorViewModels.CreateColorViewModel


@section Scripts{


}
<div class="card card-custom">
    <div class="card-header">
        <h3 class="card-title">
            Size Color
        </h3>
    </div>
    <!--begin::Form-->
    <form class="form" method="post">
        <div class="card-body">
            <div asp-validation-summary="All"></div>
            <div class="form-group row">
                <label class="col-form-label text-right col-lg-3 col-sm-12" asp-for="Title"></label>
                <div class=" col-lg-4 col-md-9 col-sm-12">
                    <input class="form-control form-control-solid name" asp-for="Title" placeholder="Title" />
                    <span asp-validation-for="Title"></span>
                </div>
            </div>

        </div>
        <div class="card-footer">
            <button type="submit" class="btn btn-primary mr-2">Submit</button>
            <button type="reset" class="btn btn-secondary">Cancel</button>
        </div>
    </form>
</div>